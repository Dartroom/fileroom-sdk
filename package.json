{
  "name": "fileroom-sdk",
  "version": "0.1.0-beta",
  "description": "A package to interact with the fileroom-api for both the browser and nodejs.",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/browser/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest  --detectOpenHandles",
    "build:node": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap --legacy-output --out-dir dist",
    "build:browser": "webpack --config webpack.config.js --mode production",
    "build:browser:dev": "webpack --config webpack.config.js --mode development --watch",
    "build:node:watch": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap --legacy-output --out-dir dist --watch",
    "build": "npm run build:node && npm run build:browser",
    "lint": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dartroom/fileroom-sdk.git"
  },
  "keywords": [],
  "author": "Spencer <spencer@dartroom.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dartroom/fileroom-sdk/issues"
  },
  "homepage": "https://github.com/dartroom/fileroom-sdk#readme",
  "dependencies": {
    "cross-fetch": "^3.1.6",
    "esbuild-plugin-d.ts": "^1.1.0",
    "isomorphic-ws": "^5.0.0",
    "react": "^18.2.0",
    "tus-js-client": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@swc/core": "^1.3.62",
    "@types/jest": "^29.5.2",
    "babel-loader": "^9.1.2",
    "esbuild": "^0.17.19",
    "jest": "^29.5.0",
    "npm-dts": "^1.3.12",
    "prettier": "2.8.8",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.3",
    "webpack-dev-server": "^4.15.0"
  }
}
